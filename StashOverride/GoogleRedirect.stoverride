name: Google search redirect
desc: Redirect Google searches from google.cn to google.com
category: enhance
icon: https://fastly.jsdelivr.net/gh/Koolson/Qure@master/IconSet/Color/Google_Search.png
http:
  mitm:
    - '*.g.cn'
    - '*.google.cn'
  url-rewrite:
    - ^https?://(?:www\.)?(?:g\.cn|google\.cn) https://www.google.com 307